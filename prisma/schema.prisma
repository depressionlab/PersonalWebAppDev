generator client {
  binaryTargets = ["native", "darwin-arm64"]
  provider        = "prisma-client-js"
  output          = "./generated/client"
  previewFeatures = ["deno"]
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model People {
  id   Int    @id @default(autoincrement())
  name String
  age  Int
}
